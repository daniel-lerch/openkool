{
  "name": "daniel-lerch/openkool",
  "description": "Open source fork of kOOL - Online church organization tool",
  "require-dev": {
    "behat/behat": "^3.4",
    "behat/mink-extension": "^2.0",
    "behat/mink-goutte-driver": "^1.2",
    "behat/mink-selenium2-driver": "^1.3",
    "emuse/behat-html-formatter": "^0.1.0"
  },
  "autoload": {
    "psr-4": {
      "LPC\\LpcEsr\\CashManagement\\": "inc/CashManagement",
      "kOOL\\Payment\\": "inc/Payment",
      "Kingsquare\\": "inc/mt940"
    }
  },
  "require": {
    "php": ">=7.2.0",
    "ext-curl": "*",
    "ext-gd": "*",
    "ext-gettext": "*",
    "ext-iconv": "*",
    "ext-imap": "*",
    "ext-json": "*",
    "ext-libxml": "*",
    "ext-mbstring": "*",
    "ext-mysqli": "*",
    "ext-openssl": "*",
    "ext-pdo": "*",
    "ext-simplexml": "*",
    "ext-xml": "*",
    "ext-xmlreader": "*",
    "ext-xmlwriter":  "*",
    "ext-zlib": "*",
    "mika56/spfcheck": "^1",
    "phpseclib/phpseclib": "~2.0",
    "phpoffice/phpspreadsheet": "^1.6",
    "phpoffice/phpword": "v0.16.*",
    "smarty/smarty": "2.6.*"
  }
}
